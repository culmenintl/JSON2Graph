{
    "name": "surge-web",
    "version": "0.2.4",
    "scripts": {
        "dev": "vite",
        "api:build": "tsc --project api",
        "swa": "vite build --mode development && yarn api:build && swa start dist --api-location api",
        "build": "yarn tsc && vite build && yarn api:build && cp staticwebapp.config.json dist",
        "preview": "vite preview",
        "test": "vitest",
        "clean": "rm -rf dist node_modules && rm -rf api/dist api/node_modules",
        "roll": "changelog -p -f 'public/CHANGELOG.md' && git add 'public/CHANGELOG.md' && git commit -m 'updated CHANGELOG.md' && yarn version --patch && git push --follow-tags"
    },
    "license": "MIT",
    "dependencies": {
        "@antv/g6": "^4.8.17",
        "@antv/graphin": "^2.7.19",
        "@antv/graphin-components": "^2.4.1",
        "@antv/graphin-icons": "^1.0.0",
        "@azure/openai": "^1.0.0-beta.6",
        "@formkit/auto-animate": "0.8.0",
        "@headlessui/react": "^1.7.2",
        "@heroicons/react": "^2.0.11",
        "@udecode/zustood": "^2.0.0",
        "immer": "10.0.2",
        "js-worker-search": "^2.0.0",
        "lodash": "^4.17.21",
        "marked": "^9.0.3",
        "notistack": "^3.0.1",
        "react": "^18.2.0",
        "react-daisyui": "^4.1.0",
        "react-dom": "^18.2.0",
        "react-hotkeys-hook": "^4.4.1",
        "react-icons": "^4.3.1",
        "zustand": "^4.4.0"
    },
    "engines": {
        "yarn": "~1.22.19",
        "node": ">=18.0.0"
    },
    "engineStrict": true,
    "devDependencies": {
        "@tailwindcss/typography": "^0.5.9",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/react-hooks": "^8.0.1",
        "@testing-library/user-event": "^14.4.3",
        "@types/color": "^3.0.3",
        "@types/js-worker-search": "^1.4.3",
        "@types/lodash": "^4.14.198",
        "@types/node": "^20.4.0",
        "@types/react": "^18.2.14",
